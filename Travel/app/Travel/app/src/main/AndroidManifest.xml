<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.chr.travel">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_BACKGROUND_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Travel">
        <activity android:name=".mpackage.LocationAccessActivity">

        </activity>
        <activity android:name=".mpackage.MapActivity">

        </activity>
        <activity android:name=".mpackage.NotificationActivity" />
        <activity android:name=".mpackage.GroupActivity">

        </activity>
        <activity android:name=".GetApiHashKeyActivity"></activity>
        <activity android:name=".login.BirthPickerActivity" />
        <activity android:name=".login.FindIdActivity" />
        <activity android:name=".login.FindPwdActivity" />
        <activity android:name=".home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.LoginActivity">

        </activity>
        <activity android:name=".login.LogoActivity">

        </activity>
        <activity android:name=".mpackage.ManagerAddGroupActivity">

        </activity>
        <activity android:name=".mpackage.PackageManagerActivity">

        </activity>
        <activity android:name=".login.SignupActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".mpackage.ManagerLocationSendActivity"/>

        <!-- ??? -->
       <!-- <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/plus" />-->
        <!-- ??? -->
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/black" />

        <!--kakao API-->
        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="5388dfebf1555a140f1277f2222bb5cc" />

        <!-- firebase -->
        <service
            android:name="service.fcm.FireBaseMessagingService"
            android:exported="false"
            tools:ignore="Instantiatable">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service android:name="service.fcm.FirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <service android:name="service.location.LocationService"
            android:enabled="true"
            android:exported="true"/>

    </application>

</manifest>