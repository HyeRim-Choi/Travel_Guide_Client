<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chr.travel">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_BACKGROUND_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Travel">
        <activity android:name=".GetLocationPraActivity">

        </activity>
        <activity android:name=".BirthPickerActivity" />
        <activity android:name=".MemberSendLocationActivity" />
        <activity android:name=".ManagerGetLocationActivity" />
        <activity android:name=".LocationActivity" />
        <activity android:name=".LocationAccessActivity" />
        <activity android:name=".FindIdActivity" />
        <activity android:name=".FindPwdActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogoActivity" />
        <activity android:name=".ManagerAddGroupActivity" />
        <activity android:name=".PackageManagerActivity" />
        <activity android:name=".SignupActivity"></activity>
        <activity android:name=".MainActivity" >

        </activity>

        <service

            android:name=".FireBaseMessagingService">

            <intent-filter>

                <action android:name="com.google.firebase.MESSAGING_EVENT"/>

            </intent-filter>

        </service>



        <service

            android:name=".FirebaseInstanceIDService">

            <intent-filter>

                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>

            </intent-filter>

        </service>

    </application>

</manifest>