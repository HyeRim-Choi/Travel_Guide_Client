<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.chr.travel">


    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_BACKGROUND_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Travel">

        <activity android:name=".SocketClientActivity"></activity>
        <activity android:name=".GroupActivity" />
        <activity android:name=".GetLocationPraActivity" />
        <activity android:name=".BirthPickerActivity" />
        <activity android:name=".MemberSendLocationActivity" />
        <activity android:name=".ManagerGetLocationActivity" />
        <activity android:name=".LocationActivity" />
        <activity android:name=".LocationAccessActivity" />
        <activity android:name=".FindIdActivity">

        </activity>
        <activity android:name=".FindPwdActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".LoginActivity">

        </activity>
        <activity android:name=".LogoActivity">

        </activity>
        <activity android:name=".ManagerAddGroupActivity" />
        <activity android:name=".PackageManagerActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <service android:name="fcm.FireBaseMessagingService"
            tools:ignore="Instantiatable">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service
            android:name="fcm.FirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
    </application>

</manifest>